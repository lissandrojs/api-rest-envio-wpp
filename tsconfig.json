{
  "compilerOptions": {
    "target": "es2016",     
    "module": "commonjs",
    "lib":["ES6"],
    "allowJs":true,
    "outDir": "build",
    "rootDir": "src", 
    "esModuleInterop": true,                  
    "forceConsistentCasingInFileNames": true,         
    "strict": true,
    "strictPropertyInitialization": false,
    "noImplicitAny": true,
    "resolveJsonModule": false,                                    
    "skipLibCheck": true        
  }
}
